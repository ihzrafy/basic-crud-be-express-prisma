generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/auth.db"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL_AUTH_DB")
}

model users {
  id       Int    @id @default(autoincrement())
  username String @unique @db.VarChar(50)
  password String
  email    String @unique @db.VarChar(100)
}
